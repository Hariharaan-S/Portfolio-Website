<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= name %></title>
    <script src="https://kit.fontawesome.com/c1b3701aff.js" crossorigin="anonymous"></script>
    <script src="https://unpkg.com/boxicons@2.1.4/dist/boxicons.js"></script>
    <link rel="stylesheet" href="css//services.css">
</head>

<body>
    <section id="service_name">
        <div class="overlay"></div>
        <div class="section_back">
            <h1>It's <%= name %>
            </h1>
        </div>
    </section>
    <section id="about_services">
        <h2 class="heading">About <span><%= name %></span></h2>
        </h2>
        <div class="about_para">
            <%= about %>
        </div>
    </section>
    <section id="services_course">
        <h2>Courses I did:</h2>
        <table>
            <% for(var i=0;i<courses.length;i++){ %>
                <tr><td><%= courses[i] %></td></tr>
            <% } %>
        </table>
    </section>
    <section id="services_project">
        <h2>Projects I Did:</h2>
        <table>
            <% for(var i=0;i<projects.length;i++){ %>
                <tr><td><%= projects[i] %></td></tr>
            <% } %>
        </table>
    </section>
    <button type="button" onclick="window.location.href='/'">Back</button>
</body>

</html>